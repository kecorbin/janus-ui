<div class="card">
  <div class="header">
    <div class="col-md-8">
      <h4 class="title">{{title}}</h4>
    </div>

  </div>
  <div class="content">
    <div class="table-responsive table-full-width">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>Type</th>
            <th>Destination</th>
            <th>Processed</th>
            <th>Started</th>
            <th>Ended</th>
            <!-- <th>Last Result</th> -->

          </tr>
        </thead>
        <tbody>
          {{#each scans as |scan|}}
            <tr>
               <td>{{scan.id}}</td>
               <td>{{scan.type}}</td>
               <td>{{scan.destination}}</td>
               <td>{{scan.processed}}</td>
               <td>{{scan.created}}</td>
               <td>{{scan.modified}}</td>
               <!-- <td>{{scan.results}}</td> -->

            </tr>


          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
