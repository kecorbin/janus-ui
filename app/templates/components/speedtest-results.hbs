<div class="card">
  <div class="header">
    <h4 class="title">{{title}}</h4>
  </div>
  <div class="content">
    <div class="table-responsive table-full-width">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>ISP</th>
            <th>Download</th>
            <th>Upload</th>
            <th>Latency</th>
            <th>Sponsor</th>

          </tr>
        </thead>
        <tbody>
          {{#each speedtests as |speedtest|}}
           <tr>
              <td>{{speedtest.id}}</td>
              <td>{{speedtest.results.isp}}</td>
              <td>{{speedtest.results.download}}</td>
              <td>{{speedtest.results.upload}}</td>
              <td>{{speedtest.results.latency}}</td>
              <td>{{speedtest.results.sponsor}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
