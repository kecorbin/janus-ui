<div class="card">
  <div class="header">
    <div class="col-md-8">
      <h4 class="title">{{title}}</h4>
    </div>
  </div>
  <div class="col-md-4">
    {{#link-to 'speedtests.add'}}<button class="btn btn-primary" type="button">New Speedtest</button> {{/link-to}}
  </div>
  <div class="content">
    <div class="table-responsive table-full-width">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>ISP</th>
            <th>Download</th>
            <th>Upload</th>
            <th>Latency</th>
            <th>Sponsor</th>

          </tr>
        </thead>
        <tbody>
          {{#each speedtests as |speedtest|}}
           <tr>
              <td>{{speedtest.id}}</td>
              <td>{{speedtest.results.isp}}</td>
              <td>{{speedtest.results.download}}</td>
              <td>{{speedtest.results.upload}}</td>
              <td>{{speedtest.results.latency}}</td>
              <td>{{speedtest.results.sponsor}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
