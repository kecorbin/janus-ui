<div class="card">
  <div class="header">
    <div class="col-md-8">
      <h4 class="title">{{title}}</h4>
    </div>
  </div>
  <div class="content">
    <div class="table-responsive table-full-width">
      <table class="table table-hover table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>Remote Device</th>
            <th>Remote Port</th>
            <th>Created</th>
            <th>Duration</th>
            <th>Secured for</th>


          </tr>
        </thead>
        <tbody>
          {{#each tunnels as |tunnel|}}
            {{#if tunnel.processed}}


                <tr>
                  <td>{{tunnel.id}}</td>
                  <td>{{tunnel.remotehost}}</td>
                  <td>{{tunnel.remoteport}}</td>
                  <td>{{tunnel.created}}</td>
                  <td>{{tunnel.timeout}} minutes</td>
                  <td>{{tunnel.sourceip}}</td>

                </tr>
            {{/if}}


          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
{{outlet}}
